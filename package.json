{
	"name": "sms-frontend-sv-v2",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "npx prettier --write .",
		"format:css": "node format-css.mjs",
		"format:css-only": "node format-css-only.mjs",
		"format-all": "npx prettier --write . && npm run format:css"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.15.3",
		"autoprefixer": "^10.4.21",
		"cross-env": "^7.0.3",
		"glob": "^11.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.5.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@lucide/svelte": "^0.507.0",
		"chart.js": "^4.5.0",
		"moment": "^2.30.1",
		"pdfjs-dist": "^5.3.31",
		"uploadthing": "^7.7.2",
		"zod": "^3.24.3"
	}
}
